{
  "name": "@appliedblockchain/tsql",
  "version": "6.7.0",
  "description": "Transact-SQL template combinators.",
  "type": "module",
  "module": "./mjs/index.js",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./mjs/index.js",
      "require": "./cjs/index.js"
    },
    "./*.js": {
      "import": "./mjs/*.js",
      "require": "./cjs/*.js"
    },
    "./*": {
      "require": "./cjs/*.js"
    }
  },
  "scripts": {
    "test": "make test",
    "preversion": "make test",
    "postversion": "make postversion"
  },
  "keywords": [
    "sql",
    "tsql",
    "transact-sql",
    "mssql",
    "microsoft-sql"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@appliedblockchain/eslint-config": "3.1.1",
    "@swc/core": "1.3.62",
    "@swc/jest": "0.2.26",
    "@tsconfig/node18": "2.0.1",
    "@types/debug": "4.1.8",
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "@types/tedious": "4.0.9",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "debug": "4.3.4",
    "eslint": "8.42.0",
    "jest": "29.5.0",
    "npm-check": "6.0.1",
    "tedious": "16.1.0",
    "typescript": "5.1.3"
  }
}
