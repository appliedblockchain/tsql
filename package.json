{
  "name": "@appliedblockchain/tsql",
  "version": "6.4.0",
  "description": "Transact-SQL template combinators.",
  "module": "./mjs/index.js",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./mjs/index.js",
      "require": "./cjs/index.js"
    },
    "./*.js": {
      "import": "./mjs/*.js",
      "require": "./cjs/*.js"
    },
    "./*": {
      "require": "./cjs/*.js"
    }
  },
  "scripts": {
    "test": "make test",
    "preversion": "make test",
    "postversion": "make postversion"
  },
  "keywords": [
    "sql",
    "tsql",
    "transact-sql",
    "mssql",
    "microsoft-sql"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@appliedblockchain/eslint-config": "3.1.1",
    "@tsconfig/node16": "1.0.3",
    "@types/debug": "4.1.7",
    "@types/jest": "29.1.1",
    "@types/tedious": "4.0.9",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "debug": "4.3.4",
    "eslint": "8.24.0",
    "jest": "29.1.2",
    "npm-check": "6.0.1",
    "tedious": "15.1.0",
    "typescript": "4.8.4"
  }
}
