{
  "name": "@appliedblockchain/tsql",
  "version": "5.5.2",
  "description": "Transact-SQL template combinators.",
  "main": "index.js",
  "scripts": {
    "test": "eslint . && tsc && jest",
    "update": "npm-check --update --save-exact",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "sql",
    "tsql",
    "transact-sql",
    "mssql",
    "microsoft-sql"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@appliedblockchain/eslint-config": "3.0.0",
    "@tsconfig/node16": "1.0.2",
    "@types/debug": "4.1.7",
    "@types/jest": "27.4.0",
    "@types/tedious": "4.0.7",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "debug": "4.3.3",
    "eslint": "8.8.0",
    "jest": "27.5.1",
    "npm-check": "5.9.2",
    "tedious": "14.3.0",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "@appliedblockchain"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "eslintIgnore": [
    "flow-typed/*",
    "wip/*",
    "*.js"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
