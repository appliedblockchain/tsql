{
  "name": "@appliedblockchain/tsql",
  "version": "5.1.1",
  "description": "Transact-SQL template combinators.",
  "main": "index.js",
  "scripts": {
    "test": "eslint . && tsc --noEmit && jest",
    "update": "npm-check --update --save-exact",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "sql",
    "tsql",
    "transact-sql",
    "mssql",
    "microsoft-sql"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@appliedblockchain/eslint-config": "3.0.0",
    "@tsconfig/node14": "1.0.0",
    "@types/debug": "4.1.5",
    "@types/jest": "26.0.23",
    "@types/tedious": "4.0.3",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "debug": "4.3.1",
    "eslint": "7.26.0",
    "jest": "26.6.3",
    "npm-check": "5.9.2",
    "tedious": "11.0.8",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "@appliedblockchain"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "eslintIgnore": [
    "flow-typed/*",
    "wip/*",
    "*.js"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
