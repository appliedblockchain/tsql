{
  "name": "@appliedblockchain/tsql",
  "version": "6.3.1",
  "description": "Transact-SQL template combinators.",
  "module": "./mjs/index.js",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./mjs/index.js",
      "require": "./cjs/index.js"
    },
    "./*.js": {
      "import": "./mjs/*.js",
      "require": "./cjs/*.js"
    },
    "./*": {
      "require": "./cjs/*.js"
    }
  },
  "scripts": {
    "test": "make test",
    "preversion": "make test",
    "postversion": "make postversion"
  },
  "keywords": [
    "sql",
    "tsql",
    "transact-sql",
    "mssql",
    "microsoft-sql"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@appliedblockchain/eslint-config": "3.0.0",
    "@tsconfig/node16": "1.0.2",
    "@types/debug": "4.1.7",
    "@types/jest": "27.4.1",
    "@types/tedious": "4.0.7",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "debug": "4.3.4",
    "eslint": "8.14.0",
    "jest": "28.0.2",
    "npm-check": "5.9.2",
    "tedious": "14.4.0",
    "typescript": "4.6.3"
  }
}
